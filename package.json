{
  "name": "vuex-listener",
  "version": "1.1.0",
  "description": "A global Vuex listener plugin",
  "main": "dist/plugin.js",
  "types": "./src/index.d.ts",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "tsc -p ./tsconfig.build.json",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "lint:fix": "tslint --fix -c tslint.json -p tsconfig.json"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TimurKiyivinski/vuex-listener.git"
  },
  "keywords": [
    "vue",
    "vuex",
    "listener"
  ],
  "author": "Timur Kiyui",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TimurKiyivinski/vuex-listener/issues"
  },
  "homepage": "https://github.com/TimurKiyivinski/vuex-listener#readme",
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/lodash": "^4.14.123",
    "husky": "^1.3.1",
    "jest": "^24.4.0",
    "lint-staged": "^8.1.5",
    "lodash": "^4.17.11",
    "ts-jest": "^24.0.0",
    "tslint": "^5.13.1",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "vue": "^2.6.12",
    "vuex": "^3.5.1"
  }
}
